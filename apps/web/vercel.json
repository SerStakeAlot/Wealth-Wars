{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_DEMO_ONLY": "true",
    "NEXT_PUBLIC_DEMO_URL": "https://demo.wealthwars.io",
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/**": {
      "includeFiles": "**"
    }
  },
  "regions": ["iad1"],
  "github": {
    "silent": true
  }
}
